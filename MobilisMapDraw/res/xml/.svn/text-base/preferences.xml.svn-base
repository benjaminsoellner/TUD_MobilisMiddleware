<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
	android:title="@string/app_name">

	<PreferenceCategory android:title="@string/pref_xmpp_server">

		<EditTextPreference android:key="pref_xmpp_host"
			android:dialogTitle="@string/pref_xmpp_host_dialogtitle"
			android:summary="@string/pref_xmpp_host_summary" android:title="@string/pref_xmpp_host_title" />

		<EditTextPreference android:key="pref_xmpp_port"
			android:dialogTitle="@string/pref_xmpp_port_dialogtitle"
			android:summary="@string/pref_xmpp_port_summary" android:title="@string/pref_xmpp_port_title" />

		<EditTextPreference android:key="pref_xmpp_service"
			android:dialogTitle="@string/pref_xmpp_service_dialogtitle"
			android:summary="@string/pref_xmpp_service_summary" android:title="@string/pref_xmpp_service_title" />



	</PreferenceCategory>



	<PreferenceCategory android:title="@string/pref_xmpp_credentials">
		<EditTextPreference android:key="pref_xmpp_user"
			android:dialogTitle="@string/pref_xmpp_user_dialogtitle"
			android:title="@string/pref_xmpp_user_title" android:summary="@string/pref_xmpp_user_summary"></EditTextPreference>
		<EditTextPreference android:key="pref_xmpp_password"
			android:dialogTitle="@string/pref_xmpp_password_dialogtitle"
			android:title="@string/pref_xmpp_password_title" android:summary="@string/pref_xmpp_password_summary"></EditTextPreference>
		<EditTextPreference android:key="pref_xmpp_resource"
			android:dialogTitle="@string/pref_xmpp_resource_dialogtitle"
			android:summary="@string/pref_xmpp_resource_summary" android:title="@string/pref_xmpp_resource_title" />
	</PreferenceCategory>
</PreferenceScreen>